{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "run-s \"build:*\"",
        "build:css": "npm run generate:css -- --style=compressed",
        "build:remix": "remix build",
        "dev": "run-p \"dev:*\"",
        "dev:css": "npm run generate:css -- --watch",
        "dev:remix": "remix dev",
        "generate:css": "sass styles/:app/styles/",
        "start": "remix-serve build",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@remix-run/node": "^1.7.6",
        "@remix-run/react": "^1.7.6",
        "@remix-run/vercel": "^1.7.6",
        "@vercel/analytics": "^0.1.5",
        "@vercel/node": "^2.7.0",
        "npm-run-all": "^4.1.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.57.1"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.7.6",
        "@remix-run/eslint-config": "^1.7.6",
        "@remix-run/serve": "^1.7.6",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "eslint": "^8.28.0",
        "typescript": "^4.9.3"
    },
    "engines": {
        "node": ">=14"
    }
}
